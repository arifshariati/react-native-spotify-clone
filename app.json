{
  "expo": {
    "name": "react-native-spotify-clone",
    "slug": "react-native-spotify-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/spotify-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle":"dark",
    "splash": {
      "image": "./assets/images/spotify-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
